{
    "name": "humanik/package-name",
    "autoload": {
        "psr-4": {
            "Humanik\\PackageName\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Humanik\\PackageName\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "friendsofphp/php-cs-fixer": "^2.16",
        "vimeo/psalm": "^4.1"
    },
    "scripts": {
        "inspect": "psalm --show-info=true --no-cache",
        "fix-style": "php-cs-fixer fix --config .php_cs.dist",
        "check-style": "php-cs-fixer fix --config .php_cs.dist --diff --dry-run"
    }
}
